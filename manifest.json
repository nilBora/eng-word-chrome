 {
  "manifest_version": 2,
  "name": "English Save Words",
  "version": "1.0",
  
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
	"contextMenus",
    "http://eng.word.local.com/*"
  ],

  "browser_action": {
	"default_title": "English Save Words",
	"default_icon": "128.png",
	"default_popup": "popup.html"
   }
}